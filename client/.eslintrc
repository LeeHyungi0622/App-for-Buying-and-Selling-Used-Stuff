{
    // 기본 eslint가 코드를 해석하지 않고,
    // babel-eslint가 코드를 해석하기 때문에 
    // 최신 문법도 에러를 발생시키지 않는다.
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env":{
        "browser": true,
        "node": true,
        "es6": true
    },
    // recommend는 코드에 대한 규제가 약하기 때문에 airbnb
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks"
    ],
    "rules": {
        
    }
}